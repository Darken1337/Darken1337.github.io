      <script>
$(document).ready(function() {
  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "1500",
      productName = "–í–µ–¥–µ–Ω–∏–µ —Å–æ—Ü—Å–µ—Ç–µ–π, 15 –ø—É–±–ª–∏–∫–∞—Ü–∏–π –ø–æ–¥ –∫–ª—é—á",
      productId = "smm15-turnkey";
    $.getJSON('https://bot.icoach.io/wayforpay-sign.php?amount=' + amount + '&currency=USD&productName=' + productName + '&productId=' + productId, function (data) {
      $.each(data, function (key, val) {
        $('.paybutton-smm15-turnkey').append('<input type="hidden" value="' + val + '"' + 'name="' + key + '">');
      });
    });
  }
  price();

  if ($('.paybutton-smm15-turnkey').length > 1) {
    alert('Duplicate product ID: smm15-turnkey');
  }
})
</script>
