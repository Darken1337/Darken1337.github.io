$(document).ready(function() {
  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "1050",
      productName = "–¢–µ—Å—Ç–æ–≤–∞—è —Ä–µ–∫–ª–∞–º–Ω–∞—è –∫–∞–º–ø–∞–Ω–∏—è –≤ –§–µ–π—Å–±—É–∫ + –ò–Ω—Å—Ç–∞–≥—Ä–∞–º –ø–æ–¥ –∫–ª—é—á",
      productId = "target-start-fb-turnkey";
    $.getJSON('https://bot.icoach.io/wayforpay-sign.php?amount=' + amount + '&currency=USD&productName=' + productName + '&productId=' + productId, function (data) {
      $.each(data, function (key, val) {
        $('.paybutton-target-start-fb-turnkey').append('<input type="hidden" value="' + val + '"' + 'name="' + key + '">');
      });
    });
  }
  price();

  if ($('.paybutton-target-start-fb-turnkey').length > 1) {
    alert('Duplicate product ID: target-start-fb-turnkey');
  }
})
</script>
