  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "1875",
      productName = "4 –Ω–µ–¥–µ–ª–∏ –≤–µ–¥–µ–Ω–∏—è —Ä–µ–∫–ª–∞–º–Ω–æ–π –∫–∞–º–ø–∞–Ω–∏–∏ –ø–æ–¥ –∫–ª—é—á",
      productId = "target-monthly-all-turnkey";
    $.getJSON('https://bot.icoach.io/wayforpay-sign.php?amount=' + amount + '&currency=USD&productName=' + productName + '&productId=' + productId, function (data) {
      $.each(data, function (key, val) {
        $('.paybutton-target-monthly-all-turnkey').append('<input type="hidden" value="' + val + '"' + 'name="' + key + '">');
      });
    });
  }
  price();

  if ($('.paybutton-target-monthly-all-turnkey').length > 1) {
    alert('Duplicate product ID: target-monthly-all-turnkey');
  }
})
</script>
<form method="POST" action="https://secure.wayforpay.com/pay" class="paybutton-target-monthly-all-turnkey" style="display:flex;flex-direction:column;">
  
