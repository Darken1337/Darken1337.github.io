<script>
$(document).ready(function() {
  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "2000",
      productName = "–í–æ—Ä–æ–Ω–∫–∞ –ø—Ä–æ–¥–∞–∂ –°—Ç–∞—Ä—Ç –ø–æ–¥ –∫–ª—é—á",
      productId = "funnel-start-turnkey";
    $.getJSON('https://bot.icoach.io/wayforpay-sign.php?amount=' + amount + '&currency=USD&productName=' + productName + '&productId=' + productId, function (data) {
      $.each(data, function (key, val) {
        $('.paybutton-funnel-start-turnkey').append('<input type="hidden" value="' + val + '"' + 'name="' + key + '">');
      });
    });
  }
  price();

  if ($('.paybutton-funnel-start-turnkey').length > 1) {
    alert('Duplicate product ID: funnel-start-turnkey');
  }
})
</script>

