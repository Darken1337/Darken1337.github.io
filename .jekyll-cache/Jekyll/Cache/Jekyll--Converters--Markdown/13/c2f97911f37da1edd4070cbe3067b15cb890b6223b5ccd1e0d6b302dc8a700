<h1 id="—Å–∫–æ–ª—å–∫–æ-—Å—Ç–æ–∏—Ç-2"><strong>–°–∫–æ–ª—å–∫–æ —Å—Ç–æ–∏—Ç</strong></h1>

<p>–ü–æ–¥ –∫–ª—é—á $9000</p>

<script>
$(document).ready(function() {
  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "6000",
      productName = "–í–æ—Ä–æ–Ω–∫–∞ –ø—Ä–æ–¥–∞–∂ –ü—Ä–µ–º–∏—É–º –ø–æ–¥ –∫–ª—é—á",
      productId = "funnel-premium-turnkey";
    $.getJSON('https://bot.icoach.io/wayforpay-sign.php?amount=' + amount + '&currency=USD&productName=' + productName + '&productId=' + productId, function (data) {
      $.each(data, function (key, val) {
        $('.paybutton-funnel-premium-turnkey').append('<input type="hidden" value="' + val + '"' + 'name="' + key + '">');
      });
    });
  }
  price();

  if ($('.paybutton-funnel-premium-turnkey').length > 1) {
    alert('Duplicate product ID: funnel-premium-turnkey');
  }
})
</script>

<form method="POST" action="https://secure.wayforpay.com/pay" class="paybutton-funnel-premium-turnkey" style="display:flex;flex-direction:column;">
  
  <input class="btn card-link btn-primary" type="submit" value="–í–Ω–µ—Å—Ç–∏ —á–∞—Å—Ç–∏—á–Ω—É—é –ø—Ä–µ–¥–æ–ø–ª–∞—Ç—É $6000" />
</form>

<hr />

<p>–í –ø–∞—Ä—Ç–Ω—ë—Ä—Å—Ç–≤–µ $4500 + 30% –æ—Ç –ø—Ä–∏–±—ã–ª–∏</p>

