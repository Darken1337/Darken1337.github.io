  $.ajaxSetup({ cache: false });

  function price() {
    let  
      amount = "1050",
      productName = "Email-—Ä–∞—Å—Å—ã–ª–∫–∞ –ø–æ–¥ –∫–ª—é—á",
      productId = "warmup-email-turnkey";
